  background-image: url("<%= image_path('ic_keyboard_arrow_left_black_24px.svg') %>") !important;
  left: 4px; 	
}

.mddtp-picker-date .mddtp-picker__right {
  /*background: url("../images/ic_keyboard_arrow_right_black_24px.svg");*/
